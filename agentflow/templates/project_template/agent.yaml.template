meta:
  id: {{ agent_id }}
  name: {{ agent_name }}
  version: 1.0.0
  author: {{ author }}
  icon: ðŸ¤–
  category: general
  description: {{ description }}

interfaces:
  inputs:
    - name: input
      type: string
      required: true
      description: Input text
      default: ""

  outputs:
    - name: output
      type: string
      description: Output result

protocols:
  {% if 'mcp' in protocols %}
  mcp:
    enabled: true
    servers: []
  {% endif %}

  {% if 'a2a' in protocols %}
  a2a:
    enabled: true
    endpoint: http://localhost:8000
    skills:
      - name: process
        description: Process input data
  {% endif %}

  {% if 'agui' in protocols %}
  agui:
    enabled: true
    stream_events: true
  {% endif %}

dependencies:
  agents: []
  tools: []
  packages: []

pocketflow:
  entry_point: main.py
  flow_name: MainFlow

visual:
  color: "#3B82F6"
  ports:
    input: [100, 50]
    output: [300, 50]
