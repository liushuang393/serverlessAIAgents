from typing import Optional

class Settings(BaseSettings):
    openai_api_key: Optional[str] = None
    default_model: str = "gpt-3.5-turbo"

    class Config:
        env_prefix = "AI_BLOCKS_"
        env_file = ".env"

def get_settings() -> Settings:
    return Settings()
````

