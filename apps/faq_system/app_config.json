{
  "name": "faq_system",
  "display_name": "FAQ „Ç∑„Çπ„ÉÜ„É†",
  "description": "Á§æÂÜÖFAQÊ§úÁ¥¢„ÉªSQLÂàÜÊûê„ÉªÂñ∂Ê•≠Ë≥áÊñôÁîªÂÉèÁîüÊàê„ÇíÁµ±Âêà„Åó„ÅüAI„ÉÅ„É£„ÉÉ„Éà„Ç∑„Çπ„ÉÜ„É†",
  "business_base": "knowledge",
  "product_line": "faq",
  "surface_profile": "business",
  "audit_profile": "business",
  "version": "2.0.0",
  "icon": "üí¨",
  "ports": {
    "api": 8005,
    "frontend": 3004,
    "db": 5433,
    "redis": null
  },
  "entry_points": {
    "api_module": "apps.faq_system.main:app",
    "health": "/api/health"
  },
  "agents": [
    {
      "name": "FAQAgent",
      "module": "agentflow.agents.faq_agent",
      "capabilities": [
        "faq",
        "rag",
        "sql",
        "chart",
        "suggestion"
      ],
      "business_base": "knowledge",
      "pattern": "specialist"
    },
    {
      "name": "SalesAgent",
      "module": "agentflow.agents.sales_agent",
      "capabilities": [
        "sales",
        "design_skills",
        "image_generation"
      ],
      "business_base": "interaction",
      "pattern": "specialist"
    },
    {
      "name": "EnhancedFAQAgent",
      "module": "apps.faq_system.backend.agents.enhanced_faq_agent",
      "capabilities": [
        "faq",
        "rag",
        "sql",
        "rich_response",
        "citation"
      ],
      "business_base": "knowledge",
      "pattern": "specialist"
    },
    {
      "name": "InternalKBAgent",
      "module": "apps.faq_system.backend.agents.internal_kb_agent",
      "capabilities": [
        "faq",
        "rag",
        "rbac",
        "conservative_mode",
        "ticket"
      ],
      "business_base": "knowledge",
      "pattern": "specialist"
    },
    {
      "name": "ExternalKBAgent",
      "module": "apps.faq_system.backend.agents.external_kb_agent",
      "capabilities": [
        "faq",
        "rag",
        "external_kb"
      ],
      "business_base": "knowledge",
      "pattern": "specialist"
    },
    {
      "name": "MaintenanceAgent",
      "module": "apps.faq_system.backend.agents.maintenance_agent",
      "capabilities": [
        "maintenance",
        "impact_analysis",
        "documentation"
      ],
      "business_base": "operations",
      "pattern": "specialist"
    },
    {
      "name": "AnalyticsAgent",
      "module": "apps.faq_system.backend.agents.analytics_agent",
      "capabilities": [
        "sql",
        "chart",
        "analysis",
        "trend"
      ],
      "business_base": "reasoning",
      "pattern": "analyzer"
    }
  ],
  "services": {
    "rag": {
      "collections": [
        "faq_knowledge"
      ],
      "enabled": true,
      "pattern": "faq_precision",
      "chunking": {
        "strategy": "sentence",
        "size": 500,
        "overlap": 80
      },
      "retrieval": {
        "method": "hybrid",
        "reranker": "cohere",
        "top_k": 8,
        "score_threshold": 0.25
      },
      "data_sources": [
        {
          "type": "database",
          "uri": "sqlite+aiosqlite:///./app.db",
          "label": "faq_system SQL source",
          "enabled": true,
          "options": {
            "dialect": "sqlite",
            "database_type": "sqlite",
            "read_mode": "query",
            "query": "SELECT 1 AS sample_value",
            "row_limit": 200
          }
        }
      ],
      "indexing_schedule": null
    },
    "sql": {
      "dialect": "postgresql"
    },
    "suggestion": {
      "enabled": true
    },
    "vector_db": {
      "provider": "qdrant",
      "url": null,
      "collection": "faq_knowledge"
    }
  },
  "dependencies": {
    "database": "postgresql",
    "redis": false,
    "external": [
      "comfyui",
      "qdrant",
      "vector_store"
    ]
  },
  "runtime": {
    "urls": {
      "backend": "http://localhost:8005",
      "frontend": "http://localhost:3004",
      "health": "http://localhost:8005/api/health",
      "database": "postgresql+asyncpg://faq:faq_password@localhost:5433/faq_system"
    },
    "database": {
      "kind": "postgresql",
      "url": "postgresql+asyncpg://faq:faq_password@localhost:5433/faq_system",
      "host": "localhost",
      "port": 5433,
      "name": "faq_system",
      "user": "faq",
      "password": null,
      "password_env": "FAQ_DB_PASSWORD",
      "note": "„É≠„Éº„Ç´„É´ÈñãÁô∫Êó¢ÂÆöÂÄ§Ôºàdocker-compose.ymlÔºâ„ÄÇ"
    },
    "commands": {
      "backend_dev": "python apps/faq_system/scripts/dev.py",
      "frontend_dev": "cd apps/faq_system/frontend && npm run dev",
      "publish": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
      "start": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
      "stop": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down"
    }
  },
  "tags": [
    "chat",
    "faq",
    "rag",
    "sales",
    "text2sql"
  ],
  "contracts": {
    "auth": {
      "enabled": true,
      "providers": [
        "api_key"
      ],
      "allow_anonymous": false,
      "required_scopes": [
        "api"
      ],
      "session_ttl_minutes": 60
    },
    "rag": {
      "enabled": true,
      "pattern": "faq_precision",
      "provider": "qdrant",
      "collections": [
        "faq_knowledge"
      ],
      "data_sources": [
        {
          "type": "database",
          "uri": "sqlite+aiosqlite:///./app.db",
          "label": "faq_system SQL source",
          "enabled": true,
          "options": {
            "dialect": "sqlite",
            "database_type": "sqlite",
            "read_mode": "query",
            "query": "SELECT 1 AS sample_value",
            "row_limit": 200
          }
        }
      ],
      "chunk_strategy": "sentence",
      "chunk_size": 500,
      "chunk_overlap": 80,
      "retrieval_method": "hybrid",
      "embedding_model": null,
      "rerank_model": "cohere",
      "default_top_k": 8,
      "score_threshold": 0.25,
      "indexing_schedule": null
    },
    "skills": {
      "auto_install": false,
      "hot_reload": true,
      "allowed_sources": [],
      "default_skills": []
    },
    "release": {
      "strategy": "manual",
      "targets": [],
      "environments": [
        "dev"
      ],
      "require_approval": true
    }
  },
  "plugin_bindings": [
    {
      "id": "official.enterprise-connector-pack",
      "version": "1.0.0",
      "config": {
        "connectors": [
          "confluence",
          "sharepoint"
        ]
      }
    },
    {
      "id": "official.industry-dictionary-pack",
      "version": "1.0.0",
      "config": {
        "locale": "ja-JP"
