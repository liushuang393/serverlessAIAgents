  "business_base": "interaction",
  "product_line": "assistant",
  "surface_profile": "business",
  "audit_profile": "business",
  "security_mode": "approval_required",
  "version": "1.0.0",
  "icon": "üì®",
  "ports": {
    "api": 8004,
    "frontend": null,
    "db": null,
    "redis": null
  },
  "entry_points": {
    "api_module": "apps.messaging_hub.main:app",
    "health": "/health"
  },
  "agents": [
    {
      "name": "PersonalAssistantCoordinator",
      "module": "apps.messaging_hub.coordinator",
      "capabilities": ["coordination", "intent_routing", "summary"],
      "business_base": "interaction",
      "pattern": "coordinator"
    },
    {
      "name": "FileOrganizerAgent",
      "module": "apps.messaging_hub.agents.file_organizer_agent",
      "capabilities": ["file_organization", "categorization"],
      "business_base": "operations",
      "pattern": "specialist"
    },
    {
      "name": "MeetingAgent",
      "module": "apps.messaging_hub.agents.meeting_agent",
      "capabilities": ["meeting", "calendar", "notes"],
      "business_base": "interaction",
      "pattern": "specialist"
    }
  ],
  "services": {
    "chatbot": {
      "skill": "ChatBotSkill"
    },
    "websocket": {
      "hub": "WebSocketHub"
    },
    "gateway": {
      "channels": [
        "telegram",
        "slack",
        "discord",
        "teams",
        "whatsapp",
        "signal"
      ]
    }
  },
  "dependencies": {
    "database": null,
    "redis": false,
    "external": ["telegram_api", "slack_api", "discord_api"]
  },
  "runtime": {
    "urls": {
      "backend": "http://localhost:8004",
      "frontend": null,
      "health": "http://localhost:8004/health",
      "database": null
    },
    "database": {
      "kind": null,
      "url": null,
      "host": null,
      "port": null,
      "name": null,
      "user": null,
      "password": null,
      "password_env": null,
      "note": null
    },
    "commands": {
      "backend_dev": "python apps/messaging_hub/scripts/dev.py",
      "frontend_dev": "cd apps/messaging_hub/admin_ui && npm run dev",
      "publish": "docker compose -f docker-compose.yml up -d --build",
      "start": "docker compose -f docker-compose.yml up -d",
      "stop": "docker compose -f docker-compose.yml down"
    },
    "cli": {
      "preferred": [
        "codex",
        "claude"
      ],
      "codex": {
        "executable": "codex",
        "install_commands": [
          [
            "npm",
            "install",
            "-g",
            "@openai/codex"
          ]
        ],
        "auth": {
          "status": [
            "codex",
            "login",
            "status"
          ],
          "api_key_env": "OPENAI_API_KEY",
          "api_key_login": [
            "codex",
            "login",
            "--with-api-key"
          ],
          "interactive_login": [
            "codex",
            "login"
          ]
        },
        "diagnostic_mode": "read_only",
        "diagnostic_command": [
          "codex",
          "exec",
          "--skip-git-repo-check",
          "--sandbox",
          "read-only"
        ]
      },
      "claude": {
        "executable": "claude",
        "install_commands": [
          [
            "npm",
            "install",
            "-g",
            "@anthropic-ai/claude-code"
          ]
        ],
        "auth": {
          "status": [
            "claude",
            "auth",
            "status",
            "--json"
          ],
          "api_key_env": "ANTHROPIC_API_KEY",
          "api_key_login": null,
          "interactive_login": [
            "claude",
            "auth",
            "login"
          ]
        },
        "diagnostic_mode": "plan",
        "diagnostic_command": [
          "claude",
          "-p",
          "--permission-mode",
          "plan"
        ]
      }
    }
  },
  "tags": [
    "messaging",
    "chatbot",
    "telegram",
    "slack",
    "discord",
    "multi-platform"
  ],
  "contracts": {
